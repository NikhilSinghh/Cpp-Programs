#include <iostream>

using namespace std;

int main()
{
     cout<<"Enetr array";
    int a[5];
    for(int i=0;i<5;i++)
    cin>>a[i];
    
     for(int i=0;i<5;i++)
        {
            for(int j=i;j<5;j++)
            {
                if(a[j]<a[i])
                {
                    int t=a[j];
                    a[j]=a[i];
                    a[i]=t;
                }
            }
        }
        
          for(int i=0;i<5;i++)
            cout<<a[i]<<" "; 
    return 0;
}
